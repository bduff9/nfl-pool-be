{
  "name": "nfl-pool-be",
  "version": "0.0.3",
  "description": "NFL confidence pool, back-end only.",
  "keywords": [
    "aswnn",
    "serverless",
    "typescript",
    "NFL",
    "graphql"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "api/graphql.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-be"
  },
  "scripts": {
    "build": "tsc",
    "env": "sync-dotenv",
    "gql:download": "apollo client:download-schema src/schema.graphql --endpoint=http://localhost:4000/dev/graphql",
    "license": "license-check-and-add add -f license-check-and-add-config.json",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "prepare": "husky install",
    "setup": "npx typesync && npm i && npm prune",
    "start": "vercel dev",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter'",
    "test:coverage": "jest --coverage --env=jsdom",
    "test:watch": "jest --watch",
    "typeCheck": "tsc",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune",
    "vercel-build": "tsc && typeorm migration:run"
  },
  "config": {},
  "dependencies": {
    "@saeris/apollo-server-vercel": "0.4.0",
    "@sentry/node": "6.5.0",
    "@sentry/tracing": "6.5.0",
    "aws-sdk": "2.919.0",
    "class-validator": "0.13.1",
    "dynamoose": "2.7.3",
    "email-templates": "8.0.7",
    "graphql": "15.5.0",
    "handlebars": "4.7.7",
    "mjml": "4.9.3",
    "mysql2": "2.2.5",
    "node-sass": "6.0.0",
    "reflect-metadata": "0.1.13",
    "tslog": "3.2.0",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.33",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@azure/functions": "1.2.3",
    "@types/email-templates": "8.0.3",
    "@types/eslint": "7.2.13",
    "@types/jest": "26.0.23",
    "@types/mjml": "4.7.0",
    "@types/node": "15.6.1",
    "@types/node-sass": "4.11.1",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.28.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettierx": "0.18.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "6.0.0",
    "jest": "27.0.3",
    "license-check-and-add": "4.0.2",
    "lint-staged": "11.0.0",
    "npm-package-json-lint": "5.1.0",
    "scriptlint": "2.1.5",
    "sync-dotenv": "2.6.2",
    "ts-jest": "27.0.2",
    "typescript": "4.3.2",
    "vercel": "23.0.0"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "git add"
    ],
    "*.{md,json}": [
      "prettierx --write",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix",
      "npmPkgJsonLint .",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>setupTests.js"
    ]
  }
}
