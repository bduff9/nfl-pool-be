{
  "name": "nfl-pool-be",
  "version": "0.0.3",
  "description": "NFL confidence pool, back-end only.",
  "keywords": [
    "aswnn",
    "serverless",
    "typescript",
    "NFL",
    "graphql"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "api/graphql.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-be"
  },
  "scripts": {
    "env": "sync-dotenv",
    "gql:download": "apollo client:download-schema src/schema.graphql --endpoint=http://localhost:4000/dev/graphql",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "setup": "npx typesync && npm i && npm prune",
    "start": "vercel dev",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter'",
    "test:coverage": "jest --coverage --env=jsdom",
    "test:watch": "jest --watch",
    "typeCheck": "tsc",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune",
    "vercel-build": "tsc && typeorm migration:run"
  },
  "config": {},
  "dependencies": {
    "@saeris/apollo-server-vercel": "0.4.0",
    "@sentry/node": "6.2.1",
    "@sentry/tracing": "6.2.1",
    "class-validator": "0.13.1",
    "graphql": "15.5.0",
    "jwt-simple": "0.5.6",
    "mysql2": "2.2.5",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.31"
  },
  "devDependencies": {
    "@types/eslint": "7.2.6",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "eslint": "7.21.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettierx": "0.17.1",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-package-json-lint": "5.1.0",
    "scriptlint": "2.1.2",
    "sync-dotenv": "2.6.2",
    "ts-jest": "26.5.2",
    "typescript": "4.2.2",
    "vercel": "21.3.2"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "husky": {
    "hooks": {
      "post-merge": "npm run setup",
      "pre-commit": "lint-staged && npm run env",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --fix",
      "git add"
    ],
    "*.{md,json}": [
      "prettierx --write",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix",
      "npmPkgJsonLint ."
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>setupTests.js"
    ]
  }
}
